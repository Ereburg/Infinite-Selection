      <section class="col-12 col-md-8 col-lg-6 col-xl-3 browse-filter">
        <h3 class="infinite-text-title browse-top-line">
          Filters
        </h3>
        <div class="filter-brands">
          <h4 class="infinite-header-small">Brands</h4>
          <div class="btn-group btn-group-toggle browse-select" data-toggle="buttons">
            <label class="btn browse-button brands-button">
              <input type="checkbox" class="browse-checkbox" name="options" id="brand1"
                     autocomplete="off" checked> Adidas
            </label>
            <label class="btn browse-button brands-button">
              <input type="checkbox" class="browse-checkbox" name="options" id="brand2"
                     autocomplete="off"> Nike
            </label>
            <label class="btn browse-button brands-button">
              <input type="checkbox" class="browse-checkbox" name="options" id="brand3"
                     autocomplete="off"> Balenciaga
            </label>
            <label class="btn browse-button brands-button">
              <input type="checkbox" class="browse-checkbox" name="options" id="brand4"
                     autocomplete="off" checked> Yeezy
            </label>
            <label class="btn browse-button brands-button">
